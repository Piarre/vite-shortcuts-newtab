{
  "manifest_version": 3,
  "name": "Chrome Shortcuts",
  "version": "1.0.0",
  "description": "Chrome Extension to replace the new tab page with a shortcuts page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon@16px.png",
    "32": "icons/icon@32px.png",
    "48": "icons/icon@48px.png",
    "128": "icons/icon@128px.png"
  }
}
